{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_area": "navbar",

    "theme_icons": [
      {
        "light": "dark.png",
        "dark": "light.png",
        "size": 512
      }
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "description": "Toolbar button action"
    },
    "opennewtab": {
      "description": "Open newtab in temp container"
    },
    "openinsame": {
      "description": "Open newtab in same container as current tab"
    },
    "multiopen": {
      "description": "Multi-Open current tab url in N temp container tabs"
    }
  },
  "description": "Open the page,tab,link or bookmark in a new temporary container tab",
  "icons": {
    "512": "icon.svg"
  },
  "manifest_version": 2,
  "name": "Open in Temp Container",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage",
    "contextualIdentities",
    "cookies",
    "menus",
    "tabs"
  ],
  "optional_permissions": ["bookmarks", "history"],
  "version": "1.1.38"
}
